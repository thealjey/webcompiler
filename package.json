{
  "name": "webcompiler",
  "version": "3.8.1",
  "description": "Lint, type-check, compile, package and gzip JavaScript (ES6 + Flow static types + JSX), for the browser as well as NodeJS; lint, compile, auto-prefix, minify and gzip SASS.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4.1.1",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha --root src -- -R nyan",
    "docs-build": "babel-node bin/docs",
    "lint": "eslint ./",
    "build": "babel-node bin/build",
    "release": "release",
    "postpublish": "git push origin `git subtree split --prefix docs master`:gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thealjey/webcompiler.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src/typedef.js",
    "lib",
    "config",
    "interfaces"
  ],
  "keywords": [
    "lint",
    "typecheck",
    "compile",
    "package",
    "uglify",
    "minify",
    "es6",
    "javascript",
    "sass",
    "jsx",
    "flow",
    "gzip",
    "jsdoc"
  ],
  "author": "Eugene Kuzmenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thealjey/webcompiler/issues"
  },
  "homepage": "https://thealjey.github.io/webcompiler",
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-member-expression-literals": "^6.5.0",
    "babel-plugin-transform-merge-sibling-variables": "^6.5.0",
    "babel-plugin-transform-property-literals": "^6.5.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-remove-debugger": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "cheerio": "^0.20.0",
    "eslint": "^2.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-flow-vars": "^0.1.3",
    "eslint-plugin-lodash": "^1.0.5",
    "eslint-plugin-react": "^3.16.1",
    "fb-watchman": "^1.9.0",
    "ink-docstrap": "^1.1.1",
    "js-yaml": "^3.5.3",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.0",
    "marked": "^0.3.5",
    "memory-fs": "^0.3.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "node-sass-import-once": "^1.2.0",
    "postcss": "^5.0.16",
    "react": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "tiny-lr": "^0.2.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.6",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "mt-changelog": "^0.6.2",
    "proxyquire": "^1.7.4",
    "release-script": "^1.0.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
